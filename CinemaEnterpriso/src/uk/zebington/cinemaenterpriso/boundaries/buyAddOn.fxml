<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox maxWidth="300.0" prefWidth="300.0" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox fx:id="itemsContainer" alignment="CENTER" spacing="10.0" />
      <HBox alignment="CENTER_RIGHT" spacing="10.0">
         <children>
            <Button onAction="#cancel" mnemonicParsing="false" text="Cancel" />
            <Button onAction="#buyAddOns" mnemonicParsing="false" text="Add To Cart" />
         </children>
      </HBox>
   </children>
</VBox>
